{
  "name": "@sentry/webpack-plugin",
  "description": "Official webpack plugin for Sentry",
  "keywords": [
    "sentry",
    "sentry-cli",
    "webpack",
    "source-map"
  ],
  "author": "Sentry",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "git@github.com:getsentry/sentry-webpack-plugin.git",
  "homepage": "https://github.com/getsentry/sentry-webpack-plugin",
  "main": "src/index.js",
  "scripts": {
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@sentry/cli": "^1.26.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "printWidth": 90
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
